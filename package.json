{
  "name": "AwesomeProject",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android:dev:debug": "react-native run-android --mode=devdebug --appIdSuffix=dev",
    "android:dev:release": "cd android && ./gradlew assembleDevRelease",
    "android:staging:debug": "react-native run-android --mode=stagingDebug --appIdSuffix=staging",
    "android:staging:release": "cd android && ./gradlew assembleStagingRelease",
    "android:prod:debug": "react-native run-android --mode=productionDebug --appIdSuffix=production",
    "android:prod:release": "cd android && ./gradlew assembleProductionRelease",
    "android:staging:codepush": "appcenter codepush release-react -a faisalur.rehman-dubizzlelabs.com/Learning-Staging -d Staging",
    "android:prod:codepush": "appcenter codepush release-react -a faisalur.rehman-dubizzlelabs.com/Learning-Staging -d Production",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start --experimental-debugger",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-firebase/app": "^19.2.2",
    "@react-native-firebase/messaging": "^19.2.2",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-code-push": "^8.2.2",
    "react-native-config": "^1.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.73.1",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.6.3",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "18.2.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
